<link
  rel="stylesheet"
  type="text/css"
  href="<%= assetPath('/css/header.css') %>"
/>
<header>
  <div id="header-left">
    <a href="/">PLACEMENT CELL</a>
  </div>
  <div id="header-right">
    <ul>
      <% if(!locals.employee){ %>
      <li><a href="/employees/sign-in">Log In</a></li>
      <li><a href="/employees/sign-up">Register</a></li>
      <% }else{ %>
      <li>Hi <%=locals.employee.name%>!</li>
      <li><a href="/employees/sign-out">Log Out</a></li>
      <% } %>
    </ul>
  </div>
</header>
